{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "^5.2.0",
        "barryvdh/laravel-debugbar": "^2.2",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "anhskohbo/no-captcha": "^3.0",
        "predis/predis": "~1.0",
        "zircote/swagger-php": "^2.0",
        "simplesoftwareio/simple-qrcode": "1.3.*",
        "tymon/jwt-auth": "1.0.0-rc.5",
        "firebase/php-jwt": "^5.0",
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/inflector": "1.1.0",
        "h2akim/pdfwatermarker": "dev-master",
        "elasticsearch/elasticsearch": "^6.0",
        "lucadegasperi/oauth2-server-laravel": "5.1.*",
        "doctrine/dbal": "~2.3",
        "grpc/grpc": "^v1.3.0",
        "google/protobuf": "^v3.3.0",
        "lustre/php-dfa-sensitive": "^1.2",
        "guzzlehttp/guzzle": "^6.3",
        "peinhu/aetherupload-laravel": "^1.0",
        "spatie/pdf-to-image": "^1.8",
        "gufy/pdftohtml-php": "~2",
        "hhxsv5/laravel-s": "^3.4",
        "chumper/zipper": "^1.0",
        "nmred/kafka-php": "v0.2.0.8",
        "malkusch/lock": "^2.1",
        "yurunsoft/yurun-lock": "dev-master",
        "yuanlj/kibana-logger" : "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "GPBMetadata\\": "GPBMetadata/",
            "Xuexitest\\": "Xuexitest/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://repo.huaweicloud.com/repository/php/"
        },
        "kibana-log": {
            "type": "git",
            "url": "git@gitlab.ops.kingnet.com:oa_web_dev/kibana-logger.git"
        }
    }
}
